{
  "name": "Figments",
  "version": "1.0",
  "description": "The unofficial Figma plugins manager",
  "permissions": ["activeTab", "declarativeContent", "storage"],
  "content_scripts": [
    {
      "matches": [
        "https://www.figma.com/file/*/*"
      ],
      "css": [
        "figments.css"
      ],
      "js": [
        "figments.js"
      ]
    }
  ],
  "icons": {
    "16": "images/plug16.png",
    "32": "images/plug32.png",
    "48": "images/plug48.png",
    "128": "images/plug128.png"
  },
  "manifest_version": 2
}
